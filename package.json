{
  "name": "meteo-station-domotique",
  "description": "Station Météo connectée",
  "title": "Station Météo",
  "date": "2021-12-23",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "ng serve --hmr --open --port=8080",
    "test": "jest --clearCache && jest --ci",
    "prebuild": "ts-node ./deployment/deploy-date.ts",
    "build": "ng build --configuration production --baseHref=./",
    "build:stats": "npm run build -- --stats-json && webpack-bundle-analyzer  ./docs/meteo-station/stats.json",
    "deploy": "ts-node ./deployment/deploy.ts",
    "build:open": "npm run build && http-server ./docs/meteo-station -p 8081 -c-1 -o ",
    "build:deploy": "npm run build && npm run deploy",
    "build:deploy:prod": "npm run build && npm run deploy:prod",
    "deploy:prod": "npm run deploy -- --configuration production",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@angular/service-worker": "^13.1.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-google-charts": "^2.2.2",
    "readline-sync": "^1.4.10",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@compodoc/compodoc": "^1.1.18",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "codelyzer": "^6.0.2",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ftp-deploy": "^2.4.1",
    "http-server": "^0.12.3",
    "jest": "^27.4.7",
    "jest-html-reporter": "^3.4.2",
    "jest-preset-angular": "^11.0.1",
    "prettier": "^2.5.1",
    "promise-ftp-common": "^1.1.5",
    "protractor": "~7.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
