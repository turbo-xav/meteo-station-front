{
  "name": "meteo-station-domotique",
  "description": "Station Météo connectée",
  "title": "Station Météo",
  "date": "2021-02-26",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "ng serve --hmr --open --port=8080",
    "test": "jest --clearCache && jest --ci",
    "prebuild": "ts-node ./deployment/deploy-date.ts",
    "build": "ng build --configuration production --baseHref=./",
    "build:stats": "npm run build -- --stats-json && webpack-bundle-analyzer  ./docs/meteo-station/stats.json",
    "deploy": "ts-node ./deployment/deploy.ts",
    "build:open": "npm run build && http-server ./docs/meteo-station -p 8081 -c-1 -o ",
    "build:deploy": "npm run build && npm run deploy",
    "build:deploy:prod": "npm run build && npm run deploy:prod",
    "deploy:prod": "npm run deploy -- --configuration production",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/forms": "^12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@angular/service-worker": "^12.1.1",
    "@auth0/angular-jwt": "^5.0.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-google-charts": "^2.2.2",
    "readline-sync": "^1.4.10",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "codelyzer": "^6.0.2",
    "ftp-deploy": "^2.4.1",
    "http-server": "^0.12.3",
    "jest": "^27.0.6",
    "jest-html-reporter": "^3.4.1",
    "jest-preset-angular": "^9.0.4",
    "promise-ftp-common": "^1.1.5",
    "protractor": "~7.0.0",
    "ts-node": "^10.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}